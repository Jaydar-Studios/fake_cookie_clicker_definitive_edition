          //stats screen
            //acheivement handler (part 2, go back to somewhere near line 210 for part 1)
        //Cookie Achievements
            if (allTimeCookies >= 1 && Achieved.bakeginner === "false") {
                Achieved.bakeginner = "true";
                Achieves.push("\nBakeginner (bake 1 cookie)");
                gameFunctions.inGameStuff.playChiev();
            } else if (allTimeCookies >= 10 && Achieved.overnine === "false") {
                Achieved.overnine = "true";
                Achieves.push("\nIt's Over NINE! (bake 10 cookies)");
                gameFunctions.inGameStuff.playChiev();
            } else if (allTimeCookies >= 100 && Achieved.rollDough === "false") {
                Achieved.rollDough = "true";
                Achieves.push("\nRolling In the DOUGH (bake 100 cookies)");
                gameFunctions.inGameStuff.playChiev();
            } else if (allTimeCookies >= 1000 && Achieved.approach === "false") {
                Achieved.approach = "true";
                Achieves.push("\nOh? You're approaching my cookies? (bake 1,000 c\n-ookies)");
                gameFunctions.inGameStuff.playChiev();
            } else if (allTimeCookies >= 10000 && Achieved.toFeed === "false") {
                Achieved.toFeed = "true";
                Achieves.push("\nEnough to feed a highschool (bake 10,000 cookies)");
                gameFunctions.inGameStuff.playChiev();
        //Building Achievements
            } else if (buildings >= 1 && Achieved.fixIt === "false") {
                Achieved.fixIt = "true";
                Achieves.push("\nCan We Fix It? (Purchase a building)");
                gameFunctions.inGameStuff.playChiev();
            } else if (cursors >= 1 && Achieved.squeakSqueak === "false") {
                Achieved.squeakSqueak = "true";
                Achieves.push("\nSqueak, Squeak (purchase 1 cursor)");
                gameFunctions.inGameStuff.playChiev();
            } else if (cursors >= 10 && Achieved.muchClick === "false") {
                Achieved.muchClick = "true";
                Achieves.push("\nMuch click. So wow. (Own 10 cursors)");
                gameFunctions.inGameStuff.playChiev();
            } else if (gMas >= 1 && Achieved.okBoomer === "false") {
                Achieved.okBoomer = "true";
                Achieves.push("\nOk Boomer (Purchase 1 grandma)");
                gameFunctions.inGameStuff.playChiev();
            } else if (gMas >= 10 && Achieved.famBuis === "false") {
                Achieved.famBuis = "true";
                Achieves.push("\nFamily Buisness (Own 10 grandmas)");
                gameFunctions.inGameStuff.playChiev();
            } else if (millis() >= 1 && Achieved.consPrize === "false") {
                Achieved.consPrize = "true";
                Achieves.push("\nConsolation Prize (Boot up this game)");
                gameFunctions.inGameStuff.playChiev();
            }
